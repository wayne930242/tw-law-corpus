{
  "status": "實施",
  "name": "現行法規所定貨幣單位折算新臺幣條例",
  "revision": [
    {
      "date": "1992.6.30",
      "content": {
        "1": {
          "num": 1,
          "zh": "第一條",
          "article": "為現行法規所定貨幣單位折算為新臺幣，以維持現行法規之適用，特制定本條例。"
        },
        "2": {
          "num": 2,
          "zh": "第二條",
          "article": "現行法規所定金額之貨幣單位為圓、銀元或元者，以新臺幣元之三倍折算之。"
        },
        "3": {
          "num": 3,
          "zh": "第三條",
          "article": "本條例自公布日施行。"
        }
      }
    }
  ]
}